package Hacking;

import java.util.Scanner;

public class HackProcessor {
	
	Scanner input;
	
	public HackProcessor(Scanner in){
		input = in;
	}
	
	public void process(String command, Server serv){
		if (command.substring(0,10).equals("sudo nmap ")){
			serv.find(command.substring(10));
		}
	}
	
}
